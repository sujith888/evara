<script>
  function changeProfile(profileId) {

    
    $('#profile').submit((e) =>{
        e.preventDefault();
        $.ajax({
        url: '/update_profile?userId='+profileId,
        method: "POST",
        data:$('#profile').serialize(),
        success: (response) => {
            location.reload();
        },
        });
    })

}
</script>