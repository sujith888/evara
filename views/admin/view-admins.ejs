<section class="">
    <div class="container d-flex justify-content-center">
      <div class="row w-75 mt-5">
        <div class="col-md-12">
          <h2 class="text-center">adminss List</h2>
          <form
            action="/admin/add_sub"
            method="post"
            enctype="multipart/form-data"
          >
            
          </form>
        </div>
      </div>
    </div>
  </section>
  <!-- Table section -->
  <section  mx-auto>
    <div class="card-body">
      <div class="row mt-5 mx-auto">
        <div class="table-responsive">
          <div class="table-responsive">
            <table  id="table2"class="table align-middle table-nowrap mb-0">
              <thead class="table-light">
                <tr>
                  <th class="align-middle" scope="col">No:</th>
                  <th class="align-middle" scope="col">admins Name</th>
                  <th class="align-middle" scope="col">Email</th>
                  <th class="align-middle" scope="col">Admin Role</th>
                  <th class="align-middle" scope="col">Option</th>
                </tr>
              </thead>
              <tbody>
                <% admin.forEach(function(admins,index) { %>
                <tr>
                  <td> <%=index+1 %> </td>
                  <td><%= admins.name %></td> 
                  <td><%= admins.email %></td>
                  <td><%= admins.role %></td>
                  <td  id="content">
                    
                      <%if(admins.blocked){%>
                        
                        <button onclick="unblockadmin('<%= admins._id %>')" class="btn btn-success" >UnBlock</button>
                 
                        
                      <%}
                      else{%>
                      <button  onclick="blockadmin('<%=admins._id %>')"  class="btn btn-danger" onclick="return confirm('are you want to block <%=admins.adminsname%>?')">Block</button>


              
                      <%}%>


                  </td>
    
                 
                </tr>
                <% }) %>
              </tbody>
            </table>
          </div>
        </div>
        <!-- table-responsive end// -->
      </div>
    </div>
  </section>
  <script>




function  blockadmin(adminId){
   console.log(adminId);
  $.ajax({
  
 url:"/admin/block_admin?adminId="+adminId,
 method:'PUT',
success:(response)=>{
  if(response){
    location.reload()
  }
  
    }
       

})

}


function  unblockadmin(adminId){
   console.log(adminId);
  $.ajax({
  
 url:"/admin/unblock_admin?adminId="+adminId,
 method:'PUT',
success:(response)=>{
  if(response){
    location.reload()
  }
    }
       

})

}








  </script>
  